{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.format": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2e5d42",
    "activityBar.background": "#2e5d42",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#9b7fc0",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#2e5d42",
    "statusBar.background": "#1d3b2a",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2e5d42",
    "statusBarItem.remoteBackground": "#1d3b2a",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1d3b2a",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1d3b2a99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1D3B2A"
}
